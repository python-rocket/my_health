{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "rootDir": "./",
    "moduleResolution": "node"
  },
  "include": ["main.ts", "utils/**/*.ts", "tabs/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
